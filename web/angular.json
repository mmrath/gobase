        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/uaa-admin/e2e/protractor.conf.js",
            "devServerTarget": "uaa-admin:serve"
          },
          "configurations": {
